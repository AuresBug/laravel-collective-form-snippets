{
    "name": "laravelcollective",
    "displayName": "Laravel Collective Form Snippets",
    "description": "Laravel Collective HTML Form Snippets for Visual Studio Code",
    "version": "1.0.0",
    "publisher": "HeinThuYa",
    "author": {
        "name": "Hein Thu Ya"
    },
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Hein-Thu-Ya/laravel-collective-form-snippets"
    },
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Snippets"
    ],
